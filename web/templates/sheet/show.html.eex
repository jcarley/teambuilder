<h2>Show sheet</h2>

<ul>

  <li>
    <strong>Name:</strong>
    <%= @sheet.name %>
  </li>

  <li>
    <strong>Description:</strong>
    <%= @sheet.description %>
  </li>

  <li>
    <strong>Type:</strong>
    <%= @sheet.type %>
  </li>

  <li>
    <strong>Start date:</strong>
    <%= @sheet.start_date %>
