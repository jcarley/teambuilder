  <li>
    <strong>End date:</strong>
    <%= @sheet.end_date %>
  </li>

</ul>

<%= link "Edit", to: team_sheet_path(@conn, :edit, @team, @sheet) %>
<%= link "Back", to: team_sheet_path(@conn, :index, @team) %>
